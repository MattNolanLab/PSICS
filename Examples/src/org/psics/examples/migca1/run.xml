	    <LineSet file="psics-out-f.txt" color="green"/>

		<View id="whole" xmin="-10." xmax="120." ymin="-80." ymax="20."/>

		<View id="first" xmin="5" xmax="20." ymin="-80." ymax="20."/>
		</LineGraph>
	</ViewConfig>
</PSICSRun>